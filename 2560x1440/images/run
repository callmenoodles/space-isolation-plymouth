for i in $(seq 15 70); do
    old_name="progress-${i}.png"
    new_num=$((i - 15))
    new_name="progress-${new_num}.png"
    if [ -f "$old_name" ]; then
        mv "$old_name" "$new_name"
        echo "Renamed $old_name to $new_name"
    else
        echo "Warning: $old_name not found."
    fi
done
